		"> 0.5%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"stylelint": {
		"extends": [
			"stylelint-config-standard-scss",
			"stylelint-config-standard-vue"
		],
		"rules": {
			"indentation": "tab",
			"selector-class-pattern": "^[a-z][a-z0-9_-]+$",
			"string-quotes": "single"
		}
	},
	"jest": {
		"globals": {
			"ts-jest": {
				"tsconfig": "tsconfig.json"
			}
		},
		"moduleFileExtensions": [
			"js",
			"ts",
			"json",
			"vue"
		],
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1"
		},
		"transform": {
			"^.+\\.tsx?$": "ts-jest",
			".*\\.(vue)$": "vue-jest"
		},
		"testPathIgnorePatterns": [
			"<rootDir>/cypress/",
			"<rootDir>/node_modules/",
			"<rootDir>/vendor/"
		]
	}
