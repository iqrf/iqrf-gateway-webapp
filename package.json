		"> 0.5%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"stylelint": {
		"extends": [
			"stylelint-config-standard-scss",
			"stylelint-config-standard-vue"
		],
		"rules": {
			"indentation": "tab",
			"selector-class-pattern": "^[a-z][a-z0-9_-]+$",
			"string-quotes": "single"
		}
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"ts",
			"json",
			"vue"
		],
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1"
		},
		"transform": {
			"^.+\\.tsx?$": [
				"ts-jest",
				{
					"tsconfig": "tsconfig.json"
				}
			],
			".*\\.(vue)$": "@vue/vue2-jest"
		},
		"testPathIgnorePatterns": [
			"<rootDir>/cypress/",
			"<rootDir>/node_modules/",
			"<rootDir>/vendor/"
		]
	}
