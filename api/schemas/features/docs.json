{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://apidocs.iqrf.org/openapi/iqrf-gateway-webapp/schemas/features/docs.json",
	"type": "object",
	"title": "Link to Documentation",
	"example": {
		"enabled": true,
		"url": "https://docs.iqrf.org/iqrf-gateway"
	},
	"required": [
		"enabled",
		"url"
	],
	"additionalProperties": false,
	"properties": {
		"enabled": {
			"$id": "#/properties/enabled",
			"type": "boolean",
			"title": "Documentation enablement",
			"example": false
		},
		"url": {
			"$id": "#/properties/url",
			"type": "string",
			"title": "Documentation URL",
			"example": "https://docs.iqrf.org/iqrf-gateway"
		}
	}
}
