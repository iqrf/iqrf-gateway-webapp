{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://apidocs.iqrf.org/openapi/iqrf-gateway-webapp/schemas/gatewayBriefInfo.json",
	"type": "object",
	"title": "Brief gateway information",
	"additionalProperties": false,
	"required": [
		"board",
		"gwId"
	],
	"properties": {
		"board": {
			"$id": "#/properties/board",
			"type": "string",
			"title": "Board",
			"example": "MICRORISC s.r.o. IQD-GW-01"
		},
		"gwId": {
			"$id": "#/properties/gwId",
			"oneOf": [
				{
					"type": "string"
				},
				{
					"type": "null"
				}
			],
			"title": "IQRF Gateway ID",
			"example": "02424D1D2D2E7580"
		}
	}
}
