{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://apidocs.iqrf.org/iqrf-gateway-webapp-api/schemas/wireguardTunnelState.json",
	"type": "object",
	"title": "Wireguard VPN tunnel state change",
	"required": [
		"active",
		"enabled"
	],
	"additionalProperties": false,
	"properties": {
		"active": {
			"$id": "#/properties/active",
			"title": "Tunnel active state",
			"description": "Activates or deactivates Wireguard tunnel",
			"type": "boolean",
			"example": true
		},
		"enabled": {
			"$id": "#/properties/enabled",
			"title": "Tunnel enabled state",
			"description": "Enables or disables Wireguard tunnel (on startup)",
			"type": "boolean",
			"example": true
		}
	}
}
