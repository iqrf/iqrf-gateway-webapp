{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://apidocs.iqrf.org/iqrf-gateway-webapp-api/schemas/remount.json",
	"type": "object",
	"title": "Remount FS schema",
	"additionalProperties": false,
	"required": [
		"mode"
	],
	"properties": {
		"mode": {
			"$id": "#/properties/mode",
			"title": "Filesystem remount mode",
			"description": "Filesystem can be remounted as read-only or read-write.",
			"type": "string",
			"enum": [
				"rw",
				"ro"
			],
			"example": "rw"
		}
	}
}
