{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://apidocs.iqrf.org/iqrf-gateway-webapp-api/schemas/dateTime.json",
	"type": "object",
	"title": "Gateway time information",
	"required": [
		"time"
	],
	"additionalProperties": false,
	"properties": {
		"time": {
			"$id": "#/properties/time",
			"type": "object",
			"title": "Gateway time",
			"required": [
				"timestamp",
				"sync",
				"name",
				"code",
				"offset"
			],
			"additionalProperties": false,
			"properties": {
				"timestamp": {
					"$id": "#/properties/time/properties/timestamp",
					"type": "integer",
					"title": "Timestamp",
					"example": 1609506000
				},
				"sync": {
					"$id": "#/properties/time/properties/timestamp",
					"type": "boolean",
					"title": "Synchronized",
					"example": true
				},
				"name": {
					"$id": "#/properties/timezone/properties/name",
					"type": "string",
					"title": "Timezone name",
					"example": "Europe/Prague"
				},
				"code": {
					"$id": "#/properties/timezone/properties/code",
					"type": "string",
					"title": "Timezone code",
					"example": "CET"
				},
				"offset": {
					"$id": "#/properties/timezone/properties/offset",
					"type": "string",
					"title": "Timezone offset",
					"example": "+0100"
				}
			}
		}
	}
}
