{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://apidocs.iqrf.org/iqrf-gateway-webapp-api/schemas/iqrfOsPatchUpdate.json",
	"type": "object",
	"title": "IQRF OS patch",
	"additionalProperties": false,
	"required": [
		"id",
		"interface"
	],
	"properties": {
		"id": {
			"$id": "#/properties/id",
			"type": "integer",
			"title": "IQRF OS patch ID",
			"example": 1
		},
		"interface": {
			"$id": "#/properties/interface",
			"type": "string",
			"title": "IQRF interface",
			"enum": [
				"SPI",
				"UART"
			],
			"example": "SPI"
		}
	}
}
