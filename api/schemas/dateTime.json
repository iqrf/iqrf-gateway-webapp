{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://apidocs.iqrf.org/iqrf-gateway-webapp-api/schemas/dateTime.json",
	"type": "object",
	"title": "Date and time",
	"required": [
		"dateTime",
		"timezone"
	],
	"properties": {
		"dateTime": {
			"$id": "#/properties/dateTime",
			"type": "string",
			"title": "Date and time",
			"example": "Friday 08 January 2021, 14:50:38"
		},
		"timezone": {
			"$id": "#/properties/timezone",
			"type": "string",
			"title": "Timezone",
			"example": "Europe/Prague (CET, +0100)"
		}
	},
	"additionalProperties": false
}