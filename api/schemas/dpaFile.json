{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://apidocs.iqrf.org/iqrf-gateway-webapp-api/schemas/dpaFile.json",
	"type": "object",
	"title": "DPA plug-in file schema",
	"description": "Meta information used to retrieve corresponding DPA file.",
	"required": [
		"osBuild",
		"dpa",
		"trSeries",
		"interfaceType"
	],
	"properties": {
		"osBuild": {
			"$id": "#/properties/osBuild",
			"type": "string",
			"title": "OS build",
			"description": "IQRF OS build version.",
			"example": "08C8"
		},
		"dpa": {
			"$id": "#/properties/dpa",
			"type": "string",
			"title": "DPA version",
			"example": "0414"
		},
		"trSeries": {
			"$id": "#/properties/trSeries",
			"type": "integer",
			"title": "TR series",
			"example": 36
		},
		"interfaceType": {
			"$id": "#/properties/interfaceType",
			"type": "string",
			"title": "Interface",
			"enum": [
				"SPI",
				"UART"
			],
			"example": "SPI"
		},
		"rfMode": {
			"$id": "#/properties/rfMode",
			"type": "string",
			"title": "RF mode",
			"enum": [
				"STD",
				"LP"
			],
			"example": "STD"
		}
	}
}
