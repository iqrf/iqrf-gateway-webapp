{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://apidocs.iqrf.org/openapi/iqrf-gateway-webapp/schemas/networkWireGuardTunnelPeer.json",
	"type": "object",
	"title": "WireGuard VPN tunnel peer configuration",
	"examples": [
		{
			"id": 1,
			"publicKey": "oFfa5w0nSkfYNdggb/Su/MUJQ1ryotfjMo5zfSim7yk=",
			"psk": "Tkyhh13wpqw/ffriFSQtR8TNdxyT9s2MPhqLlT5Z2SA=",
			"keepalive": 25,
			"endpoint": "vpn.example.org",
			"port": 51820,
			"allowedIPs": {
				"ipv4": [
					{
						"id": 1,
						"address": "10.0.0.0",
						"prefix": 24
					}
				],
				"ipv6": [
					{
						"id": 2,
						"address": "2001:db8::/48",
						"prefix": 48
					}
				]
			}
		}
	],
	"required": [
		"publicKey",
		"keepalive",
		"endpoint",
		"port",
		"allowedIPs",
		"tunnelId"
	],
	"additionalProperties": false,
	"properties": {
		"id": {
			"$ref": "https://apidocs.iqrf.org/openapi/iqrf-gateway-webapp/schemas/definitions/common.json#/definitions/id"
		},
		"publicKey": {
			"$ref": "https://apidocs.iqrf.org/openapi/iqrf-gateway-webapp/schemas/definitions/wireGuard.json#/definitions/publicKey"
		},
		"psk": {
			"$id": "#/properties/psk",
			"title": "WireGuard tunnel peer pre-shared key",
			"type": [
				"string",
				"null"
			],
			"example": "Tkyhh13wpqw/ffriFSQtR8TNdxyT9s2MPhqLlT5Z2SA="
		},
		"keepalive": {
			"$id": "#/properties/keepalive",
			"title": "WireGuard tunnel peer keepalive interval",
			"type": "integer",
			"minimum": 0,
			"example": 25
		},
		"endpoint": {
			"$id": "#/properties/endpoint",
			"title": "WireGuard tunnel peer endpoint",
			"type": "string",
			"example": "vpn.example.org"
		},
		"port": {
			"$ref": "https://apidocs.iqrf.org/openapi/iqrf-gateway-webapp/schemas/definitions/wireGuard.json#/definitions/port"
		},
		"allowedIPs": {
			"$id": "#/properties/allowedIPs",
			"title": "WireGuard tunnel peer allowed IP adresses",
			"type": "object",
			"required": [
				"ipv4",
				"ipv6"
			],
			"additionalProperties": false,
			"properties": {
				"ipv4": {
					"$id": "#/properties/allowedIPs/properties/ipv4",
					"title": "WireGuard tunnel peer allowed IPv4 addresses",
					"type": "array",
					"additionalItems": false,
					"items": {
						"$ref": "https://apidocs.iqrf.org/openapi/iqrf-gateway-webapp/schemas/definitions/wireGuard.json#/definitions/ipv4"
					}
				},
				"ipv6": {
					"$id": "#/properties/allowedIPs/properties/ipv6",
					"title": "WireGuard tunnel peer allowed IPv6 addresses",
					"type": "array",
					"additionalItems": false,
					"items": {
						"$ref": "https://apidocs.iqrf.org/openapi/iqrf-gateway-webapp/schemas/definitions/wireGuard.json#/definitions/ipv6"
					}
				}
			}
		},
		"tunnelId": {
			"$ref": "https://apidocs.iqrf.org/openapi/iqrf-gateway-webapp/schemas/definitions/common.json#/definitions/id"
		}
	}
}
