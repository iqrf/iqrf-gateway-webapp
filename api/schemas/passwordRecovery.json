{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://apidocs.iqrf.org/openapi/iqrf-gateway-webapp/schemas/passwordRecovery.json",
	"type": "object",
	"title": "Password recovery",
	"required": [
		"password"
	],
	"properties": {
		"password": {
			"$id": "#/properties/password",
			"type": "string",
			"title": "Password",
			"minLength": 15,
			"maxLength": 64,
			"pattern": "^[ -~]+$",
			"example": "8Yz#t>pL^sD|Uq&bW@"
		}
	},
	"additionalProperties": false
}
