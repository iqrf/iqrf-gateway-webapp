{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://apidocs.iqrf.org/openapi/iqrf-gateway-webapp/schemas/task.json",
	"type": "object",
	"title": "Task",
	"additionalProperties": false,
	"required": [
		"taskId",
		"clientId",
		"timeSpec",
		"task"
	],
	"properties": {
		"taskId": {
			"$id": "#/properties/taskId",
			"type": "string",
			"pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-4[0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$",
			"example": "5070cf2c-3d07-415f-a6e3-553d5549917f"
		},
		"clientId": {
			"$id": "#/properties/clientId",
			"type": "string",
			"title": "Scheduler messaging instance",
			"example": "SchedulerMessaging"
		},
		"description": {
			"$id": "#/properties/description",
			"title": "Description",
			"type": "string",
			"example": "Example task"
		},
		"timeSpec": {
			"$id": "#/properties/timeSpec",
			"type": "object",
			"title": "Time specification",
			"additionalProperties": false,
			"required": [
				"cronTime",
				"exactTime",
				"periodic",
				"period",
				"startTime"
			],
			"properties": {
				"cronTime": {
					"$id": "#/properties/timeSpec/cronTime",
					"title": "CRON time expression with seconds and year",
					"oneOf": [
						{
							"type": "array",
							"minItems": 7,
							"maxItems": 7,
							"additionalItems": false,
							"items": {
								"type": "string"
							},
							"example": [
								"0",
								"*/2",
								"*",
								"*",
								"*",
								"*",
								"*"
							]
						},
						{
							"type": "string",
							"example": "0 */2 * * * * *"
						}
					]

				},
				"exactTime": {
					"$id": "#/properties/timeSpec/exactTime",
					"type": "boolean",
					"title": "Exact time task",
					"example": false
				},
				"periodic": {
					"$id": "#/properties/timeSpec/periodic",
					"type": "boolean",
					"title": "Periodic task",
					"example": false
				},
				"period": {
					"$id": "#/properties/timeSpec/periodic",
					"type": "number",
					"title": "Task period",
					"example": 0
				},
				"startTime": {
					"$id": "#/properties/timeSpec/startTime",
					"type": "string",
					"title": "Exact start time",
					"example": "2020-12-31T23:59:59.999999"
				}
			}
		},
		"task": {
			"$id": "#/properties/task",
			"type": "array",
			"title": "Task(s)",
			"items": {
				"$id": "#/properties/task/items",
				"$ref": "#/definitions/task",
				"additionalItems": false
			},
			"example": [
				{
					"messaging": "MqttMessaging",
					"message": {
						"mType": "iqrfEmbedLedr_Pulse",
						"data": {
							"msgId": "testEmbedLedr",
							"req": {
								"nAdr": 2,
								"param": {}
							},
							"returnVerbose": true
						}
					}
				}
			]
		},
		"persist": {
			"$id": "#/properties/persist",
			"title": "Persistent task",
			"type": "boolean"
		},
		"enabled": {
			"$id": "#/properties/enabled",
			"title": "Launch on startup",
			"type": "boolean"
		}
	},
	"definitions": {
		"task": {
			"$id": "#/definitions/task",
			"type": "object",
			"required": [
				"messaging",
				"message"
			],
			"properties": {
				"messaging": {
					"$id": "#/definitions/task/messaging",
					"title": "Task messaging type(s)",
					"oneOf": [
						{
							"type": "string",
							"example": "MqttMessaging"
						},
						{
							"type": "array",
							"items": {
								"$id": "#/definitions/task/messaging/items",
								"type": "string",
								"pattern": "^[^&]+$"
							},
							"example": [
								"MqttMessaging"
							]
						}
					]
				},
				"message": {
					"$id": "#/definitions/task/message",
					"$ref": "#/definitions/message"
				}
			},
