{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://apidocs.iqrf.org/iqrf-gateway-webapp-api/schemas/task.json",
	"type": "object",
	"title": "Task",
	"additionalProperties": false,
	"required": [
		"taskId",
		"clientId",
		"timeSpec",
		"task"
	],
	"properties": {
		"taskId": {
			"$id": "#/properties/taskId",
			"type": "integer",
			"title": "Task ID",
			"example": 2
		},
		"clientId": {
			"$id": "#/properties/clientId",
			"type": "string",
			"title": "Scheduler messaging instance",
			"example": "SchedulerMessaging"
		},
		"timeSpec": {
			"$id": "#/properties/timeSpec",
			"type": "object",
			"title": "Time specification",
			"additionalProperties": false,
			"required": [
				"cronTime",
				"exactTime",
				"periodic",
				"period",
				"startTime"
			],
			"properties": {
				"cronTime": {
					"$id": "#/properties/timeSpec/cronTime",
					"type": "array",
					"title": "CRON time expression with seconds and year",
					"minItems": 7,
					"maxItems": 7,
					"additionalItems": false,
					"items": {
						"$id": "#/properties/timeSpec/cronTime/items",
						"type": "string"
					},
					"example": [
						"0",
						"*/2",
						"*",
						"*",
						"*",
						"*",
						"*"
					]
				},
				"exactTime": {
					"$id": "#/properties/timeSpec/exactTime",
					"type": "boolean",
					"title": "Exact time task",
					"example": false
				},
				"periodic": {
					"$id": "#/properties/timeSpec/periodic",
					"type": "boolean",
					"title": "Periodic task",
					"example": false
				},
				"period": {
					"$id": "#/properties/timeSpec/periodic",
					"type": "number",
					"title": "Task period",
					"example": 0
				},
				"startTime": {
					"$id": "#/properties/timeSpec/startTime",
					"type": "string",
					"title": "Exact start time",
					"example": "2020-12-31T23:59:59.999999"
				}
			}
		},
		"task": {
			"$id": "#/properties/task",
			"type": "array",
			"title": "Task(s)",
			"items": {
				"$id": "#/properties/task/items",
				"$ref": "#/definitions/task",
				"additionalItems": false
			},
			"example": [
				{
					"messaging": "MqttMessaging",
					"message": {
						"mType": "iqrfEmbedLedr_Pulse",
						"data": {
							"msgId": "testEmbedLedr",
							"req": {
								"nAdr": 2,
								"param": {}
							},
							"returnVerbose": true
						}
					}
				}
			]
		}
	},
	"definitions": {
		"task": {
			"$id": "#/definitions/task",
			"type": "object",
			"required": [
				"messaging",
				"message"
			],
			"properties": {
				"messaging": {
					"$id": "#/definitions/task/messaging",
					"title": "Task messaging type(s)",
					"oneOf": [
						{
							"type": "string",
							"example": "MqttMessaging"
						},
						{
							"type": "array",
							"items": {
								"$id": "#/definitions/task/messaging/items",
								"type": "string",
								"pattern": "^[^&]+$"
							},
							"example": [
								"MqMessaging",
								"MqttMessaging"
							]
						}
					]
				},
				"message": {
					"$id": "#/definitions/task/message",
					"$ref": "#/definitions/message"
				}
			},
			"additionalProperties": false
		},
