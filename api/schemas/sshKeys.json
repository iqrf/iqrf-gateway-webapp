{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://apidocs.iqrf.org/iqrf-gateway-webapp-api/schemas/sshKeys.json",
	"title": "SSH keys",
	"type": "array",
	"additionalItems": false,
	"minItems": 1,
	"items": [
		{
			"$id": "#/items",
			"type": "string",
			"pattern": "^ssh-rsa AAAA[0-9A-Za-z+/]+[=]{0,3}( [^@]+@[^@]+)?$",
			"example": "ssh-rsa  your@email.com"
		}
	]
}
