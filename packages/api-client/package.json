{
	"name": "@iqrf/iqrf-gateway-webapp-client",
	"version": "0.3.0",
	"description": "IQRF Gateway Webapp JavaScript client library",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./services": {
			"types": "./dist/services/index.d.mts",
			"import": "./dist/services/index.mjs",
			"require": "./dist/services/index.cjs"
		},
		"./services/*": {
			"types": "./dist/services/*.d.mts",
			"import": "./dist/services/*.mjs",
			"require": "./dist/services/*.cjs"
		},
		"./types": {
			"types": "./dist/types/index.d.mts",
			"import": "./dist/types/index.mjs",
			"require": "./dist/types/index.cjs"
		},
		"./types/*": {
			"types": "./dist/types/*.d.mts",
			"import": "./dist/types/*.mjs",
			"require": "./dist/types/*.cjs"
		},
		"./utils": {
			"types": "./dist/utils/index.d.mts",
			"import": "./dist/utils/index.mjs",
			"require": "./dist/utils/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"scripts": {
		"build": "NODE_OPTIONS='--max-old-space-size=4096' tsdown",
		"coverage:unit": "vitest --run --coverage",
		"dev": "tsdown --watch",
		"docs": "npm run docs:typedoc",
		"docs:typedoc": "rm -rf docs/ && typedoc",
		"prepublish": "npm run build",
		"lint": "npm run lint:js && npm run lint:ts",
		"lint:js": "eslint --cache --cache-location node_modules/.cache/eslint/ .",
		"lint:ts": "tsc --noEmit",
		"test:unit": "vitest --run"
	},
	"homepage": "https://gitlab.iqrf.org/open-source/iqrf-gateway-webapp/-/tree/master/packages/api-client",
	"bugs": {
		"url": "https://gitlab.iqrf.org/open-source/iqrf-gateway-webapp/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://gitlab.iqrf.org/open-source/iqrf-gateway-webapp.git"
	},
	"files": [
		"dist",
		"src",
		"README.md"
	],
	"keywords": [
		"iqrf",
		"iqrf-gateway",
		"iqrf-gateway-webapp",
		"api-client"
	],
	"author": {
		"name": "MICRORISC s.r.o.",
		"url": "https://www.microrisc.com/"
	},
	"contributors": [
		{
			"name": "Roman Ondráček",
			"email": "roman.ondracek@iqrf.com",
			"url": "https://www.romanondracek.cz/"
		}
	],
	"license": "Apache-2.0",
	"type": "module",
	"dependencies": {
		"@iqrf/iqrf-gateway-daemon-utils": "^0.1.17",
		"@iqrf/iqrf-ui-common-types": "^0.1.0",
		"axios": "^1.13.5",
		"luxon": "^3.7.2",
		"openapi-typescript": "^7.12.0",
		"punycode": "^2.3.1",
		"uuid": "^13.0.0"
	},
	"devDependencies": {
		"@iqrf/eslint-config": "^0.4.2",
		"@types/luxon": "^3.7.1",
		"@types/node": "^25.2.2",
		"@types/punycode": "^2.1.4",
		"@vitest/coverage-istanbul": "^4.0.18",
		"@vitest/spy": "^4.0.18",
		"axios-mock-adapter": "^2.1.0",
		"eslint": "^9.39.2",
		"ts-node": "^10.9.2",
		"tsdown": "^0.20.3",
		"typedoc": "^0.28.16",
		"typedoc-material-theme": "^1.4.1",
		"typedoc-plugin-mdn-links": "^5.1.1",
		"typescript": "^5.9.3",
		"vite-tsconfig-paths": "^6.1.0",
		"vitest": "^4.0.18"
	}
}
