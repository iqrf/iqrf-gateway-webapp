{
	"name": "iqrf/iqrf-gateway-webapp",
	"description": "IQRF Gateway Daemon web application",
	"type": "project",
	"keywords": [
		"nette",
		"iqrf",
		"iqrf-daemon",
		"iqrf-gateway",
		"iqrf-gateway-daemon"
	],
	"homepage": "https://gitlab.iqrf.org/open-source/iqrf-gateway-webapp",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "Roman Ondráček",
			"homepage": "https://romanondracek.cz/",
			"email": "roman.ondracek@iqrf.com",
			"role": "developer"
		}
	],
	"require": {
		"php": ">=8.1.0",
		"ext-json": "*",
		"ext-openssl": "*",
		"ext-posix": "*",
		"ext-zip": "*",
		"acmephp/ssl": "^2.1.0",
		"contributte/apitte": "^v0.12.1",
		"contributte/console": "^v0.9.3",
		"contributte/console-extra": "^v0.7.2",
		"contributte/guzzlette": "^v3.3.1",
		"contributte/middlewares": "^v0.12.0",
		"contributte/monolog": "^v0.5.2",
		"contributte/psr7-http-message": "^v0.10.0",
		"contributte/sentry": "^v0.2.2",
		"contributte/translation": "^v2.0.3",
		"cweagans/composer-patches": "^1.7.3",
		"darsyn/ip": "^4.1.0",
		"egulias/email-validator": "^4.0.2",
		"guzzlehttp/guzzle": "^7.8.1",
		"iqrf/iqrf-ide-macros-parser": "^v1.2.0",
		"iqrf/iqrf-repository": "dev-master",
		"justinrainbow/json-schema": "^5.2.13",
		"latte/latte": "^v3.0.12",
		"lcobucci/jwt": "^5.2.0",
		"nette/application": "^v3.1.14",
		"nette/bootstrap": "^v3.2.1",
		"nette/caching": "^v3.2.3",
		"nette/di": "^v3.1.8",
		"nette/finder": "^v3.0.0",
		"nette/http": "^v3.2.3",
		"nette/mail": "^v4.0.2",
		"nette/neon": "^v3.4.1",
		"nette/php-generator": "^v4.1.2",
		"nette/robot-loader": "^v4.0.1",
		"nette/safe-stream": "^v3.0.1",
		"nette/security": "^v3.1.8",
		"nette/utils": "v4.0.3 as v3.2.9",
		"nettrine/annotations": "^v0.7.0",
		"nettrine/cache": "^v0.3.0",
		"nettrine/dbal": "^v0.8.2",
		"nettrine/fixtures": "^v0.6.4",
		"nettrine/migrations": "^v0.9.1",
		"nettrine/orm": "^v0.8.4",
		"php-http/guzzle7-adapter": "^1.0.0",
		"phpmailer/phpmailer": "^v6.9.1",
		"ramsey/uuid": "^4.7.5",
		"ramsey/uuid-doctrine": "^2.0.0",
		"ratchet/pawl": "^v0.4.1",
		"spatie/ssl-certificate": "^2.6.2",
		"symfony/process": "^v6.4.0",
		"tracy/tracy": "^v2.10.5"
	},
	"require-dev": {
		"contributte/qa": "^v0.3.1",
		"mockery/mockery": "^1.6.7",
		"nette/tester": "^v2.5.1",
		"ninjify/nunjuck": "^v0.4.0",
		"phpstan/phpstan": "^1.10.50",
		"phpstan/phpstan-deprecation-rules": "^1.1.4",
		"phpstan/phpstan-doctrine": "^1.3.53",
		"phpstan/phpstan-mockery": "^1.1.1",
		"phpstan/phpstan-nette": "^1.2.9",
		"phpstan/phpstan-strict-rules": "^1.5.2",
		"phpstan/phpstan-symfony": "^1.3.6",
		"rector/rector": "^0.18.13"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests"
		}
	},
	"support": {
		"email": "roman.ondracek@iqrf.com",
		"issues": "https://gitlab.iqrf.org/open-source/iqrf-gateway-webapp/issues",
		"source": "https://gitlab.iqrf.org/open-source/iqrf-gateway-webapp"
	},
	"archive": {
		"exclude": [
			"/docker/",
			"/docs/",
			"/debian/",
			"/install/"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"composer-exit-on-patch-failure": true
	},
	"config": {
		"platform": {
			"php": "8.1.0"
		},
		"sort-packages": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"composer/package-versions-deprecated": true,
			"cweagans/composer-patches": true,
			"php-http/discovery": true
		}
	}
}
