{
	"name": "iqrf/iqrf-gateway-webapp",
	"description": "IQRF Gateway Daemon web application",
	"type": "project",
	"keywords": [
		"nette",
		"iqrf",
		"iqrf-daemon",
		"iqrf-gateway",
		"iqrf-gateway-daemon"
	],
	"homepage": "https://gitlab.iqrf.org/open-source/iqrf-gateway-webapp",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "Roman Ondráček",
			"homepage": "https://romanondracek.cz/",
			"email": "roman.ondracek@iqrf.com",
			"role": "developer"
		}
	],
	"require": {
		"php": ">=7.3.0",
		"ext-json": "*",
		"ext-openssl": "*",
		"ext-posix": "*",
		"ext-zip": "*",
		"acmephp/core": "^2.0.1",
		"acmephp/ssl": "^2.0.1",
		"contributte/apitte": "^v0.9.1",
		"contributte/console": "^v0.9.2",
		"contributte/console-extra": "^v0.7.1",
		"contributte/guzzlette": "^v3.2.2",
		"contributte/monolog": "^v0.5.0",
		"contributte/translation": "^v0.9.3",
		"darsyn/ip": "^4.0.2",
		"egulias/email-validator": "^3.1.2",
		"grifart/enum": "^0.2.1",
		"guzzlehttp/guzzle": "^7.4.2",
		"iqrf/iqrf-ide-macros-parser": "^v1.2.0",
		"iqrf/iqrf-repository": "dev-master",
		"justinrainbow/json-schema": "^5.2.12",
		"latte/latte": "^v2.11.3",
		"lcobucci/jwt": "^3.4.6",
		"nette/application": "^v3.1.5",
		"nette/bootstrap": "^v3.1.2",
		"nette/caching": "^v3.1.2",
		"nette/di": "^v3.0.13",
		"nette/finder": "^v2.5.3",
		"nette/http": "^v3.1.6",
		"nette/mail": "^v3.1.8",
		"nette/neon": "^v3.3.3",
		"nette/php-generator": "^v3.6.7",
		"nette/robot-loader": "^v3.4.1",
		"nette/safe-stream": "^v2.5.0",
		"nette/security": "^v3.1.5",
		"nette/utils": "^v3.2.7",
		"nettrine/annotations": "^v0.7.0",
		"nettrine/cache": "^v0.3.0",
		"nettrine/dbal": "^v0.8.0",
		"nettrine/fixtures": "^v0.6.2",
		"nettrine/migrations": "^v0.8.0",
		"nettrine/orm": "^v0.8.3",
		"php-http/guzzle7-adapter": "^1.0.0",
		"ramsey/uuid": "^4.2.3",
		"ramsey/uuid-doctrine": "^1.8.1",
		"ratchet/pawl": "^v0.4.1",
		"sentry/sdk": "^2.2.0",
		"symfony/process": "^v5.4.5",
		"tracy/tracy": "^v2.9.1"
	},
	"require-dev": {
		"mockery/mockery": "^1.5.0",
		"nette/tester": "^v2.4.2",
		"ninjify/coding-standard": "^v0.12.1",
		"ninjify/nunjuck": "^v0.3.0",
		"ninjify/qa": "^v0.13.0",
		"phpstan/phpstan": "~1.6.9",
		"phpstan/phpstan-deprecation-rules": "^1.0.0",
		"phpstan/phpstan-doctrine": "^1.3.1",
		"phpstan/phpstan-mockery": "^1.0.0",
		"phpstan/phpstan-nette": "^1.0.0",
		"phpstan/phpstan-strict-rules": "^1.1.0",
		"phpstan/phpstan-symfony": "^1.1.8"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests"
		}
	},
	"support": {
		"email": "roman.ondracek@iqrf.com",
		"issues": "https://gitlab.iqrf.org/open-source/iqrf-gateway-webapp/issues",
		"source": "https://gitlab.iqrf.org/open-source/iqrf-gateway-webapp"
	},
	"archive": {
		"exclude": [
			"/docker/",
			"/docs/",
			"/debian/",
			"/install/"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"platform": {
			"php": "7.3.0"
		},
		"sort-packages": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"composer/package-versions-deprecated": true
		}
	}
}
